<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CalendarPager">
        <attr name="currentColor" format="color" />
        <attr name="hintColor" format="color" />
        <attr name="selectColor" format="color" />
        <attr name="todayColor" format="color" />

        <attr name="textSize" format="dimension" />
        <attr name="pointSize" format="dimension" />

        <attr name="startDate" format="string" />
        <attr name="endDate" format="string" />

        <attr name="calendarHeight" format="dimension"/>
        <attr name="duration" format="integer"/>

        <attr name="firstDayOfWeek" format="string"/>
        <attr name="defaultCalendar" format="string"/>

        <attr name="backgroundColor" format="color" />

    </declare-styleable>
</resources>